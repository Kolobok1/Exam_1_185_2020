#include <iostream>
#include <ctime>

using namespace std;

class A {
private: int max;
public:
    A (int array[], int n) {
    int max = array[0];
    
}
int getMax()
{
return max;
};
int F(int n, int array[])
{
    for (int i=0; i<n; i++)
        if (max>array[i])
        max = array[i];
cout << max << endl;
};
};

int main()
{
srand(time(NULL));
int i,n;
int array[i];
cin >> n;
for ( i=0; i<n; i++)
    array[i]= rand() % 100;
A MAX(array, i);
MAX.getMax();
MAX.F(i, array);

return 0;
}
